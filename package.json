{
  "name": "tb-changelog",
  "version": "0.1.0",
  "description": "Generate changelog from git history and TrackerBoot API",
  "type": "module",
  "bin": {
    "tb-changelog": "./dist/cli.js"
  },
  "scripts": {
    "dev": "DEBUG=true tsx src/cli.ts",
    "build": "tsup",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write",
    "lint:unsafe": "biome check --write --unsafe",
    "lint:ci": "biome ci",
    "pre-commit": "npm run lint:unsafe && npm run typecheck && npm run test",
    "pre-publish": "npm run typecheck && npm run build"
  },
  "keywords": [
    "changelog",
    "git",
    "trackerboot",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bekind-Labs/tb-changelog.git"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/node": "^22.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "consola": "^3.4.2",
    "execa": "^9.6.0"
  }
}
